#!/usr/bin/env bash

rm ~/.bash_profile
ln -s ~/dotfiles/lib/bash_profile ~/.bash_profile

rm ~/.bashrc
ln -s ~/dotfiles/lib/bashrc ~/.bashrc

rm ~/.gemrc
ln -s ~/dotfiles/lib/gemrc ~/.gemrc

rm ~/.git-completion
ln -s ~/dotfiles/lib/git-completion ~/.git-completion

rm ~/.vimrc
ln -s ~/dotfiles/lib/vimrc ~/.vimrc

source ~/.bash_profile
